<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.glotaran.comparetracesfx.fxcomponents.CompareTracesCseneController">
   <top>
      <ToolBar maxHeight="-Infinity" minHeight="-Infinity" prefHeight="35.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
          <Button fx:id="tbClear" mnemonicParsing="false" onAction="#tbClearAction" onMouseEntered="#tbMouseEntered" onMouseExited="#tbMouseExited" text="Clear">
               <tooltip>
                  <Tooltip text="Clear all selected tracess" />
               </tooltip>
            </Button>
            <Button fx:id="tbExportTraces" mnemonicParsing="false" onMouseEntered="#tbMouseEntered" onMouseExited="#tbMouseExited" text="Export">
               <tooltip>
                  <Tooltip text="Export all tracess to a file" />
               </tooltip>
            </Button>
            <ToggleButton fx:id="tbOverlayTracess" mnemonicParsing="false" onMouseEntered="#tbMouseEntered" onMouseExited="#tbMouseExited" text="ToggleButton">
               <tooltip>
                  <Tooltip text="Overlay all tracess in one graph" />
               </tooltip>
            </ToggleButton>
            <Separator orientation="VERTICAL" prefHeight="200.0" />
            <Button mnemonicParsing="false" onAction="#testSingleAction" text="testSingle" />
            <Button mnemonicParsing="false" onAction="#testMultyAction" text="testMulty" />
        </items>
      </ToolBar>
   </top>
   <center>
      <ScrollPane fitToHeight="true" fitToWidth="true" BorderPane.alignment="CENTER">
         <content>
            <TilePane fx:id="graphPaneMain" hgap="2.0" prefColumns="3" vgap="2.0" />
         </content>
      </ScrollPane>
   </center>
</BorderPane>
